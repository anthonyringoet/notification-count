{
  "name": "notification-count",
  "version": "0.0.1",
  "description": "Keep track of notifications in your title (or somewhere else)",
  "author": {
    "name": "Anthony Ringoet",
    "email": "anthonyringoet@gmail.com",
    "url": "http://anthonyringoet.be"
  },
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "test-local": "browserify test/*.js | testling",
    "watch-example": "watchify example/main.js -d -o example/bundle.js -v",
    "serve-example": "http-server example",
    "prova": "prova test/*.js -b"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "keywords": [
    "notification",
    "browserify",
    "browser",
    "activity"
  ],
  "license": "MIT",
  "devDependencies": {
    "watchify": "^0.10.1",
    "browserify": "^4.1.5",
    "http-server": "^0.6.1",
    "tape": "^2.13.1",
    "prova": "^1.9.0",
    "testling": "^1.6.1"
  }
}
